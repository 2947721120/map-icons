{
  "name": "map-icons",
  "description": "",
  "version": "3.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/scottdejonge/Map-Icons.git"
  },
  "dependencies": {},
  "devDependencies": {
    "bigfishtv-turret": "^3.1.0",
    "less": "^2.5.1",
    "less-plugin-autoprefix": "^1.5.0",
    "less-plugin-clean-css": "^1.5.1",
    "uglify-js": " ^2.4.23",
    "watch": "^0.13.0",
    "browserify": "8.1.3",
    "uglify-js": " 2.4.16"
  },
  "scripts": {
    "watch:css": "watch \"npm run build:css\" \"docs/styles\"",
    "watch:js": "watch \"npm run build:js\" \"docs/scripts\"",
    "watch": "npm run watch:css & npm run watch:js",
    "build:css": "lessc --include-path=\"node_modules/bigfishtv-turret\" docs/styles/main.less --clean-css --autoprefix > docs/css/main.css",
    "build:js": "browserify docs/scripts/main.js | uglifyjs -c > docs/js/main.js",
    "build": "npm run build:css & npm run build:js"
  }
}
